model User {
    id         Int          @id @default(autoincrement())
    email      String    @unique
    password   String
    phoneNumber String?
    address String?
    birthDate  DateTime?
    expirationDateDrivingLicense DateTime?   
    getDrivingLicense   DateTime?
    createdAt  DateTime?     @default(now())
    userLogins UserLogin[]
    roles      ROLE  
    rental Rental[]
@@map("users")   
}

model UserLogin {
    id     Int  @id
    userId Int
    users   User @relation(fields: [userId], references: [id])
}
model Rental{
    id Int @id
    UserId Int?

    users   User? @relation(fields: [UserId], references: [id])
}
enum ROLE {
    USER
    GLOBAL_ADMIN
}